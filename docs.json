{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Oryx Browser",
  "description": "Oryx Browser",
  "colors": {
    "primary": "#7D56FC",
    "light": "#7D56FC",
    "dark": "#7D56FC"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": ["index", "onboarding"]
          },
          {
            "group": "AI & Automation",
            "expanded": true,
            "pages": [
              "llm-setup-guide",
              {
                "group": "Cloud AI Providers",
                "expanded": true,
                "pages": [
                  "bring-your-own-keys/gemini",
                  "bring-your-own-keys/claude",
                  "bring-your-own-keys/openai",
                  "bring-your-own-keys/openrouter"
                ]
              },
              {
                "group": "Local AI Models",
                "expanded": true,
                "pages": [
                  "local-LLMs/ollama",
                  "local-LLMs/lm-studio",
                  "local-LLMs/gpt-oss"
                ]
              },
              {
                "group": "MCP Integration",
                "expanded": false,
                "tag": "Coming Soon"
              }
            ]
          },
          {
            "group": "$ORYX Token",
            "tag": "Soon",
            "pages": ["oryx-token/overview"]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Twitter",
          "href": "https://mintlify.com/docs",
          "icon": "square-x-twitter"
        },
        {
          "anchor": "Telegram",
          "href": "https://mintlify.com/blog",
          "icon": "telegram"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "background": {
    "decoration": "windows",
    "color": {
      "light": "#F2F2F2",
      "dark": "#121212"
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Buy $ORYX",
        "href": "mailto:hi@mintlify.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Download Oryx",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "telegram": "https://linkedin.com/company/mintlify"
    }
  }
}
